{
    "manifest_version" : 2,
    "name" : "__MSG_name__",
    "version" : "0.0.4",
    "description" : "__MSG_description__",
    "default_locale" : "zh_CN",
    "omnibox" : { "keyword" : "down" },
    "icons" : {
        "16"  : "imgs/icon_16.png",
        "48"  : "imgs/icon_48.png",
        "128" : "imgs/icon_128.png"
    },
    "background" : {
        "scripts" : [
            "inc/base.js",
            "inc/thunder.js",
            "inc/notice.js",
            "inc/setting.js",
            "background.js"
        ]
    },
    "permissions" : [
        "cookies",
        "contextMenus",
        "notifications",
        "downloads",
        "storage",
        "http://*.xunlei.com/*"
    ]
}